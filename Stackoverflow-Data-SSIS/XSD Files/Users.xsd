<?xml version="1.0"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="users">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" name="row">
          <xs:complexType>
            <xs:attribute name="Id" type="xs:int" use="optional" />
            <xs:attribute name="Reputation" type="xs:int" use="optional" />
            <xs:attribute name="CreationDate" type="xs:dateTime" use="optional" />
            <xs:attribute name="DisplayName" type="xs:string" use="optional" />
            <xs:attribute name="LastAccessDate" type="xs:dateTime" use="optional" />
            <xs:attribute name="WebsiteUrl" type="xs:string" use="optional" />
            <xs:attribute name="Location" type="xs:string" use="optional" />
            <xs:attribute name="AboutMe" type="xs:string" use="optional" />
            <xs:attribute name="Views" type="xs:int" use="optional" />
            <xs:attribute name="UpVotes" type="xs:int" use="optional" />
            <xs:attribute name="DownVotes" type="xs:int" use="optional" />
            <xs:attribute name="AccountId" type="xs:int" use="optional" />
            <xs:attribute name="Age" type="xs:long" use="optional" />
            <xs:attribute name="ProfileImageUrl" type="xs:string" use="optional" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>